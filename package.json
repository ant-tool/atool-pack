{
  "name": "atool-pack",
  "version": "0.2.0",
  "description": "npm pack and extract",
  "homepage": "http://github.com/ant-tool/atool-pack",
  "author": "yaya <yaya@uloveit.com.cn>",
  "repository": {
    "type": "git",
    "url": "https://github.com/ant-tool/atool-pack.git"
  },
  "main": "./lib/index",
