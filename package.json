    "os-tmpdir": "^1.0.1",
    "rimraf": "^2.5.2",
    "shelljs": "^0.6.0",
    "tar.gz": "^1.0.3"
  },
  "devDependencies": {
    "babel-cli": "~6.6.4",
    "babel-core": "~6.7.4",
    "babel-plugin-add-module-exports": "~0.1.2",
    "babel-preset-es2015": "~6.6.0",
    "babel-preset-stage-0": "~6.5.0",
    "chai": "^3.5.0",
    "eslint": "^2.5.3",
    "eslint-config-airbnb": "^6.2.0",
    "mocha": "^2.4.5",
    "pre-commit": "^1.1.2"
  },
  "babel": {
    "presets": [
      "es2015",
      "stage-0"
    ],
    "plugins": [
      "add-module-exports"
